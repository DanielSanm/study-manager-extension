{
    "manifest": 3,
    "name": "Study Manager",
    "version": "1.0.0",
    "description": "Gerenciar os conte√∫dos virtuais do SAVA e organiza-los no Google Agenda.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}